  template:
    # terraform:
    #   backend:
    #     bucket: 'tf-state-prod'
    #     prefix: 'terraform/state'
    resource:
      google_dns_record_set:
        {{ name }}:
          managed_zone: var.{{ name }}_managed_zone
          name: var.{{ name }}_name
          rrdatas: var.{{ name }}_rrdatas
          ttl: var.{{ name }}_ttl
          type: var.{{ name }}_type
          # project: ''
    variable:
      {{ name }}_managed_zone:
        type: string
      {{ name }}_name:
        type: string
      {{ name }}_rrdatas:
        type: list
      {{ name }}_ttl:
        type: int
      {{ name }}_type:
        type: string
    output:
      id:
        value: google_dns_record_set.{{ name }}.id
      thub_id:
        value: google_dns_record_set.{{ name }}.id
      project:
        value: google_dns_record_set.{{ name }}.project
