      {{ name }}_object:
        type: string
      {{ name }}_bucket:
        type: string
      {{ name }}_entity:
        type: string
      {{ name }}_role:
        type: string
    output:
      id:
        value: google_storage_object_access_control.{{ name }}.id
      thub_id:
        value: google_storage_object_access_control.{{ name }}.id
      domain:
        value: google_storage_object_access_control.{{ name }}.domain
      email:
