    resource:
      google_compute_backend_bucket_signed_url_key:
        {{ name }}:
          backend_bucket: var.{{ name }}_backend_bucket
          key_value: var.{{ name }}_key_value
          name: var.{{ name }}_name
          # project: ''
    variable:
      {{ name }}_backend_bucket:
        type: string
      {{ name }}_key_value:
        type: string
      {{ name }}_name:
        type: string
    output:
      id:
        value: google_compute_backend_bucket_signed_url_key.{{ name }}.id
      thub_id:
        value: google_compute_backend_bucket_signed_url_key.{{ name }}.id
      project:
        value: google_compute_backend_bucket_signed_url_key.{{ name }}.project
