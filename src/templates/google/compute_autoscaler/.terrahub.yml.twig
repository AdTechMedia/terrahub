  template:
    # terraform:
    #   backend:
    #     bucket: 'tf-state-prod'
    #     prefix: 'terraform/state'
    resource:
      google_compute_autoscaler:
        {{ name }}:
          target: var.{{ name }}_target
          autoscaling_policy: 
            -
              max_replicas: 0
              min_replicas: 0
          #     cpu_utilization: 
          #       -
          #         target: ''
          #     cooldown_period: 0
          name: var.{{ name }}_name
          # description: ''
          # metric: 
          #   -
          #     name: ''
          #     type: ''
          #     target: ''
          # zone: ''
          # project: ''
    variable:
      {{ name }}_target:
        type: string
      {{ name }}_name:
        type: string
    output:
      id:
        value: google_compute_autoscaler.{{ name }}.id
      thub_id:
        value: google_compute_autoscaler.{{ name }}.id
      creation_timestamp:
        value: google_compute_autoscaler.{{ name }}.creation_timestamp
      self_link:
        value: google_compute_autoscaler.{{ name }}.self_link
      zone:
        value: google_compute_autoscaler.{{ name }}.zone
      project:
        value: google_compute_autoscaler.{{ name }}.project
