          # next_hop_ip: ''
    variable:
      {{ name }}_dest_range:
        type: string
      {{ name }}_network:
        type: string
      {{ name }}_name:
        type: string
    output:
      id:
        value: google_compute_route.{{ name }}.id
      thub_id:
        value: google_compute_route.{{ name }}.id
      project:
        value: google_compute_route.{{ name }}.project
      next_hop_network:
        value: google_compute_route.{{ name }}.next_hop_network
      self_link:
        value: google_compute_route.{{ name }}.self_link
