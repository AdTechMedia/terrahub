    resource:
      google_compute_route:
        {{ name }}:
          dest_range: var.{{ name }}_dest_range
          network: var.{{ name }}_network
          name: var.{{ name }}_name
          # tags: []
          # project: ''
          # next_hop_vpn_tunnel: ''
          # priority: 0
          # next_hop_instance_zone: ''
          # description: ''
          # next_hop_gateway: ''
