      {{ name }}_frontend_port_end:
        type: int
      {{ name }}_frontend_port_start:
        type: int
      {{ name }}_loadbalancer_id:
        type: string
      {{ name }}_protocol:
        type: string
      {{ name }}_backend_port:
        type: int
      {{ name }}_frontend_ip_configuration_name:
        type: string
      {{ name }}_name:
        type: string
