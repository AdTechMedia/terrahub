      {{ name }}_usage_plan_id:
        type: string
      {{ name }}_key_id:
        type: string
      {{ name }}_key_type:
        type: string
    output:
      id:
        value: aws_api_gateway_usage_plan_key.{{ name }}.id
      thub_id:
        value: aws_api_gateway_usage_plan_key.{{ name }}.id
      name:
        value: aws_api_gateway_usage_plan_key.{{ name }}.name
      value:
