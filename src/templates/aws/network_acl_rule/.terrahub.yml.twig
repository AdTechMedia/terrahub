          # ipv6_cidr_block: ''
          # to_port: 0
          # icmp_code: ''
          # cidr_block: ''
          # from_port: 0
          # icmp_type: ''
    variable:
      {{ name }}_protocol:
        type: string
      {{ name }}_rule_action:
        type: string
      {{ name }}_network_acl_id:
        type: string
      {{ name }}_rule_number:
        type: int
    output:
      id:
        value: aws_network_acl_rule.{{ name }}.id
      thub_id:
        value: aws_network_acl_rule.{{ name }}.id
