    resource:
      aws_network_acl_rule:
        {{ name }}:
          protocol: var.{{ name }}_protocol
          rule_action: var.{{ name }}_rule_action
          network_acl_id: var.{{ name }}_network_acl_id
          rule_number: var.{{ name }}_rule_number
