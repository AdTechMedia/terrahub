  template:
    # terraform:
    #   backend:
    #     s3:
    #       region: 'us-east-1'
    #       bucket: 'bucket-name'
    #       key: 'path/to/terraform.tfstate'
    resource:
      aws_cloudhsm2_cluster:
        {{ name }}:
          hsm_type: var.{{ name }}_hsm_type
          subnet_ids: var.{{ name }}_subnet_ids
          # tags: 
          #   '[TO_BE_REPLACED]': ''
          # source_backup_identifier: ''
    variable:
      {{ name }}_hsm_type:
        type: string
      {{ name }}_subnet_ids:
        type: set
    output:
      id:
        value: aws_cloudhsm2_cluster.{{ name }}.id
      thub_id:
        value: aws_cloudhsm2_cluster.{{ name }}.id
      vpc_id:
        value: aws_cloudhsm2_cluster.{{ name }}.vpc_id
      cluster_certificates:
        value: aws_cloudhsm2_cluster.{{ name }}.cluster_certificates
      cluster_state:
        value: aws_cloudhsm2_cluster.{{ name }}.cluster_state
      cluster_id:
        value: aws_cloudhsm2_cluster.{{ name }}.cluster_id
      security_group_id:
        value: aws_cloudhsm2_cluster.{{ name }}.security_group_id
