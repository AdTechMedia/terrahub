    resource:
      aws_security_group_rule:
        {{ name }}:
          security_group_id: var.{{ name }}_security_group_id
          type: var.{{ name }}_type
          from_port: var.{{ name }}_from_port
          to_port: var.{{ name }}_to_port
          protocol: var.{{ name }}_protocol
          # self: false
          # prefix_list_ids: []
          # cidr_blocks: []
          # ipv6_cidr_blocks: []
          # source_security_group_id: ''
          # description: ''
    variable:
      {{ name }}_security_group_id:
        type: string
      {{ name }}_type:
        type: string
      {{ name }}_from_port:
        type: int
      {{ name }}_to_port:
        type: int
      {{ name }}_protocol:
        type: string
    output:
      id:
        value: aws_security_group_rule.{{ name }}.id
      thub_id:
        value: aws_security_group_rule.{{ name }}.id
      source_security_group_id:
        value: aws_security_group_rule.{{ name }}.source_security_group_id
