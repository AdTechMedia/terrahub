    resource:
      aws_guardduty_ipset:
        {{ name }}:
          detector_id: var.{{ name }}_detector_id
          name: var.{{ name }}_name
          format: var.{{ name }}_format
          location: var.{{ name }}_location
          activate: var.{{ name }}_activate
    variable:
      {{ name }}_detector_id:
        type: string
      {{ name }}_name:
        type: string
      {{ name }}_format:
        type: string
      {{ name }}_location:
        type: string
      {{ name }}_activate:
        type: bool
    output:
      id:
        value: aws_guardduty_ipset.{{ name }}.id
      thub_id:
        value: aws_guardduty_ipset.{{ name }}.id
