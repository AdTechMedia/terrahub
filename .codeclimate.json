{
  "version": "2",
  "checks": {
    "file-lines": {
      "config": {
        "threshold": 550
      }
    },
    "method-complexity": {
      "config": {
        "threshold": 10
      }
    },
    "method-lines": {
      "config": {
        "threshold": 100
      }
    },
    "method-count": {
      "config": {
        "threshold": 35
      }
    },
    "argument-count": {
      "config": {
        "threshold": 5
      }
    },
    "similar-code": {
      "enabled": false
    }
  },
  "engines": {
    "eslint": {
      "enabled": true,
      "channel": "eslint-2",
      "config": {
        "config": ".eslintrc.json"
      }
    },
    "fixme": {
      "enabled": true
    }
  }
}
